{
  "name": "yammy-messenger",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:mock": "concurrently \"vite --mode mock\" \"json-server src/db/json-db.json -p 8000\"",
    "build": "tsc && vite build",
    "build:mock": "tsc && vite build --mode mock",
    "preview": "vite preview",
    "type-check": "tsc --build --force",
    "prettier": "prettier --ignore-unknown --write .",
    "eslint": "eslint **/*.{tsx,ts,js}",
    "eslint:fix": "eslint **/*.{tsx,ts,js} --fix",
    "lint": "npm run prettier && npm run eslint:fix && npm run type-check"
  },
  "dependencies": {
    "@tanstack/react-query": "^5.56.2",
    "@types/node": "^22.7.4",
    "axios": "^1.7.7",
    "cookies-ts": "^1.0.5",
    "flag-icons": "^7.2.3",
    "framer-motion": "^11.9.0",
    "html-react-parser": "^5.1.18",
    "json-server": "^1.0.0-beta.3",
    "lucide-react": "^0.446.0",
    "prettier": "^3.3.3",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.26.2",
    "sass": "^1.79.4",
    "zustand": "^5.0.0-rc.2"
  },
  "devDependencies": {
    "@eslint/js": "^9.9.0",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react-swc": "^3.5.0",
    "concurrently": "^9.0.1",
    "eslint": "^9.11.1",
    "eslint-plugin-react-hooks": "^5.1.0-rc.0",
    "eslint-plugin-react-refresh": "^0.4.9",
    "globals": "^15.9.0",
    "typescript": "^5.5.3",
    "typescript-eslint": "^8.0.1",
    "vite": "^5.4.1"
  }
